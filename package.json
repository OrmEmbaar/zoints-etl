    "author": "OrmEmbaar",
    "version": "0.1.0",
    "main": "src/index.js",
    "types": "src/index.d.ts",
    "module": "src/index.js",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Factoshi/zeetl.git"
    },
    "license": "MIT",
